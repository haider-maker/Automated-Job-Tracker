{
  "manifest_version": 3,
  "name": "Job Apply Tracker",
  "version": "1.0",
  "description": "Tracks when you apply to a job and sends it to the local DB.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ]
}
